# LUSUSH Test Configuration File
# This file tests the configuration loading functionality

[history]
history_enabled = true
history_size = 500
history_no_dups = true
history_timestamps = true

[completion]
completion_enabled = true
fuzzy_completion = true
completion_threshold = 70
completion_case_sensitive = false
completion_show_all = true

[prompt]
prompt_style = git
prompt_theme = test
git_prompt_enabled = true
git_cache_timeout = 3

[behavior]
multiline_edit = true
auto_cd = true
spell_correction = true
confirm_exit = true
tab_width = 8
color_scheme = dark
colors_enabled = true
verbose_errors = true
debug_mode = false

[aliases]
ll = ls -l
la = ls -la
grep = grep --color=auto
ls = ls --color=auto

[keys]
# Custom key bindings would go here
